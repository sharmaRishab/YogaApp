<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:onClick="openActivity"
    android:orientation="vertical"
    tools:context=".MainActivity8"
    tools:ignore="SpeakableTextPresentCheck">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="100dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:layout_marginTop="10dp"
            android:textAlignment="center"
            android:layout_marginBottom="10dp"/>
    </LinearLayout>



<ImageView
    android:id="@+id/i1"
    android:layout_width="80dp"
    android:layout_height="80dp"
    android:layout_marginLeft="25dp"
    android:layout_marginBottom="3dp"
    android:src="@drawable/inclinehammercurls"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Incline Hammer Curls"
        android:layout_marginLeft="150dp" />




    <View
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginLeft="25dp"
        android:background="@color/black"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/ropepushdown"/>
    <View
    android:layout_width="300dp"
    android:layout_height="2dp"
    android:layout_marginLeft="25dp"
    android:background="@color/black"/>

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/hammercurls"/>
    <View
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginLeft="25dp"
        android:background="@color/black"/>
    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:src="@drawable/singlehandtricep"/>
    <View
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginLeft="25dp"
        android:background="@color/black"/>


</LinearLayout>


